        </Expander>
        <ListBox x:Name="listvalores" HorizontalAlignment="Left" Height="293" Margin="11,162,0,0" VerticalAlignment="Top" Width="225" />
        <ListBox x:Name="listBox_Copy" HorizontalAlignment="Left" Height="293" Margin="252,162,0,0" VerticalAlignment="Top" Width="225"/>

    </Grid>
</Window>
