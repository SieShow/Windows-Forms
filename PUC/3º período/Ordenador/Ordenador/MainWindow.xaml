        <Label x:Name="label" Content="Lista Desordenada" HorizontalAlignment="Left" Margin="11,131,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy" Content="Lista Ordenada" HorizontalAlignment="Left" Margin="252,131,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtfrom" HorizontalAlignment="Left" Height="23" Margin="17,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
