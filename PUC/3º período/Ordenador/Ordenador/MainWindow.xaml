        <Button x:Name="btngerar" Content="Gerar Valores" HorizontalAlignment="Left" Margin="143,41,0,0" VerticalAlignment="Top" Width="84" Click="btngerar_Click" Height="23"/>
        <Expander x:Name="expander" Header="Exibir resultados" HorizontalAlignment="Left" Margin="270,24,0,0" VerticalAlignment="Top" Width="127">
