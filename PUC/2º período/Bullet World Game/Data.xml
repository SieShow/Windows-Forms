  <!--Cada player tem um nome e uma pontuação.-->
  <!--O personagem com quem esse player jogou não é relevante nessa Coluna-->
  <users>
    <PlayerInfo name ="Guest" points="0"/>
  </users>
  <!--RANKING DEFINIDO POR CADA PERSONAGEM-->
  <!--Assim que cada personagem for jogado por um player, seu nome será salvo junto com a sua pontuação-->
  <!--na Coluna referente ao personagem-->
