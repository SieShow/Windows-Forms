    <BulletBill>
      <player  name ="" points =" "/>
    </BulletBill>
    <CheepCheep>
      <player  name ="" points =" "/>
    </CheepCheep>
    <CrazyBlooper>
      <player  name ="" points =" "/>
    </CrazyBlooper>
    <LeftMario>
      <player  name ="" points =" "/>
    </LeftMario>
  </Characters>
</World>